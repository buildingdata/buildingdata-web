<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mobisys.building.dao.BbcHumidityDao">

    <select id="queryBbcHumidityById" resultType="com.mobisys.building.entity.BbcHumidityInfo">
        SELECT *
        FROM t_bbc_humidity
        WHERE
        station_id = #{stationId}
    </select>
</mapper>
