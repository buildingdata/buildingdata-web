<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mobisys.building.dao.TypicalWinterAirconditionerParamDao">
    <select id="queryTWAPById" resultType="com.mobisys.building.entity.TypicalWinterAirconditionerParamInfo">
        SELECT param1, param2, param3
        FROM t_typical_winter_airconditioner_param
        WHERE station_id = #{id}
    </select>

</mapper>